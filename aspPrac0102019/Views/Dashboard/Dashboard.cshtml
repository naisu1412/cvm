@model System.Data.DataTable
@{
    ViewBag.Title = "Dashboard";
}
<html>
    <body>

        <table>

            @for (int i = 0; i < Model.Rows.Count; i++)
            {
                <tr>
                    <td>@Model.Rows[i][1]</td>
                    <td>@Model.Rows[i][2]</td>
                    <td>
                        <a href="@Url.Action("Edit","Dashboard",new { @id = @Model.Rows[i][0]})">Edit</a>
                        <a href="@Url.Action("Delete","Dashboard",new { @id = @Model.Rows[i][0]})">Delete</a>
                    </td>
                </tr>

            }

        </table>

        <a href="@Url.Action("Create","Dashboard",Model)">Create</a>
    </body>
</html>
